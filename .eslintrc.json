{
  "root": true,
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": ["jest"],
  "env": {
    "jest/globals": true
  },
  "rules": {
    "react/forbid-prop-types": 0,
    "no-underscore-dangle": ["error", { "allow": ["_propsStack"] }],
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0,
    "import/no-named-as-default": 0,
    "max-len": [
      "error", {
        "code": 120,
        "ignoreComments": true
      }
    ]
  }
}
